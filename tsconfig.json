{
    "compilerOptions": {
      "outDir": "./dist",
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "removeComments": true,
      "declaration": true,
      "sourceMap": true,
      "jsx": "react",
      "typeRoots": [
        "node_modules/@types"
      ]
    },
    "include": [
      "./src/**/*.tsx",
      "./src/**/*.ts"
    ],
    "exclude": [
      "node_modules",
      "dist"
    ]
  }